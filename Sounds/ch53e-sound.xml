<?xml version="1.0"?>
<!--
	thanks to the SAR simulation (GPL) for the helicopter sounds,
	and the Urban game (LGPL) for the weapon sounds

	+x = aft
	+y = left
	+z = up
-->

<PropertyList>
    <fx>
<!--
# sim/model/ch53e/state
# 0 off
# 1 engine startup
# 2 engine startup with small torque on rotor
# 3 engine idle
# 4 engine accel
# 5 engine sound loop
-->
        <!-- inside -->
        <engine>
            <name>turbine-start-inside</name>
            <mode>once</mode>
            <path>Aircraft/ch53e/Sounds/startup_interior.wav</path>
            <condition>
                <and>
                    <or>
                        <equals>
                            <property>sim/model/ch53e/state</property>
                            <value>1</value>
                        </equals>
                        <equals>
                            <property>sim/model/ch53e/state</property>
                            <value>2</value>
                        </equals>
                        <equals>
                            <property>sim/model/ch53e/state</property>
                            <value>3</value>
                        </equals>
                    </or>
                    <property>sim/current-view/internal</property>
                </and>
            </condition>
            <volume>
                <factor>0.20</factor>
            </volume>
        </engine>

        <engine>
            <name>turbine-loop-inside</name>
            <mode>looped</mode>
            <path>Sounds/starter_loop.wav</path>
            <condition>
                <and>
                    <equals>
                        <property>sim/model/ch53e/state</property>
                        <value>2</value>
                    </equals>
                    <property>sim/current-view/internal</property>
                </and>
            </condition>
            <volume>
                <factor>0.20</factor>
            </volume>
        </engine>
        <startup_interior_continuous>
            <name>startup_interior_continuous</name>
            <mode>looped</mode>
            <path>Aircraft/ch53e/Sounds/startup_interior_continuous.wav</path>
            <condition>
                <and>
                    <or>
                        <equals>
                            <property>sim/model/ch53e/state</property>
                            <value>3</value>
                        </equals>
                        <equals>
                            <property>sim/model/ch53e/state</property>
                            <value>4</value>
                        </equals>
                    </or>
                    <property>sim/current-view/internal</property>
                </and>
            </condition>
            <volume>
                <factor>0.20</factor>
            </volume>
        </startup_interior_continuous>

