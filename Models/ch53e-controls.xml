<PropertyList>


<!-- *** Front Panel *** -->

<animation>
	<type>pick</type>
	<object-name>CautionTestButton</object-name>
	<action>
		<button>0</button>
	   <binding>
	   	<command>property-assign</command>
			<property>sim/model/ch53e/control-input/caution-test</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>sim/model/ch53e/control-input/caution-test</property>
				<value>0</value>
