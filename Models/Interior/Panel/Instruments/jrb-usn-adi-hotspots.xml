<?xml version="1.0"?>
<PropertyList>

<name>jrb-usn-adi Hotspots</name>
<background>Aircraft/ch53e/Models/Interior/Panel/Instruments/transparent.png</background>
<w>1024</w>
<h>1024</h>
<y-offset>0</y-offset>

<instruments>
	<instrument>
		<name></name>
		<x>512</x>
		<y>512</y>
		<w>1024</w>
		<h>1024</h>
		<w-base>1024</w-base>
		<h-base>1024</h-base>
		<actions>
			<action>
				<name>Roll knob dec</name>
				<button>0</button>
				<x>-448</x>
				<y>-448</y>
				<w>64</w>
				<h>128</h>
				<binding>
					<command>property-adjust</command>
					<property>instrumentation/attitude-indicator/roll-offset-deg</property>
					<step>-1.5</step>
					<min>-15</min>
					<max>15</max>
					<wrap>false</wrap>
				</binding>
			</action>
			<action>
				<name>Roll knob inc</name>
				<button>0</button>
				<x>-384</x>
				<y>-448</y>
				<w>64</w>
				<h>128</h>
				<binding>
					<command>property-adjust</command>
					<property>instrumentation/attitude-indicator/roll-offset-deg</property>
					<step>1.5</step>
					<min>-15</min>
					<max>15</max>
					<wrap>false</wrap>
				</binding>
			</action>
			<action>
				<name>Pitch knob dec</name>
				<button>0</button>
				<x>320</x>
				<y>-448</y>
				<w>64</w>
				<h>128</h>
				<binding>
					<command>property-adjust</command>
					<property>instrumentation/attitude-indicator/horizon-offset-deg</property>
					<step>-0.75</step>
					<min>-15</min>
					<max>15</max>
					<wrap>false</wrap>
				</binding>
			</action>
			<action>
				<name>Pitch knob inc</name>
				<button>0</button>
				<x>384</x>
				<y>-448</y>
				<w>64</w>
				<h>128</h>
				<binding>
					<command>property-adjust</command>
					<property>instrumentation/attitude-indicator/horizon-offset-deg</property>
					<step>0.75</step>
					<min>-15</min>
					<max>15</max>
					<wrap>false</wrap>
				</binding>
			</action>
		</actions>
	</instrument>
</instruments>

</PropertyList>
