<?xml version="1.0"?>
<PropertyList>

<name>Clock Hotspots</name>
<background>Aircraft/ch53e/Models/Interior/Panel/Instruments/transparent.png</background>
<w>1024</w>
<h>1024</h>
<y-offset>0</y-offset>

<instruments>
	<instrument>
		<name/>
		<x>512</x>
		<y>512</y>
		<w>1024</w>
		<h>1024</h>
		<w-base>1024</w-base>
		<h-base>1024</h-base>
		<actions>
			<action>
				<name>adjCrsBack</name>
				<button>0</button>
				<x>-450</x>
				<y>-460</y>
				<w>70</w>
				<h>140</h>
				<binding>
					<command>property-adjust</command>
					<property>sim/model/jrb-usn/id-249-course-deg</property>
					<step>-1</step>
					<min>0</min>
					<max>360</max>
					<wrap>1</wrap>
				</binding>
			</action>
			<action>
				<name>adjCrsForeward</name>
				<button>0</button>
				<x>-380</x>
				<y>-460</y>
				<w>70</w>
				<h>140</h>
				<binding>
					<command>property-adjust</command>
					<property>sim/model/jrb-usn/id-249-course-deg</property>
					<step>1</step>
					<min>0</min>
					<max>360</max>
					<wrap>1</wrap>
				</binding>
			</action>
		</actions>
	</instrument>
</instruments>

</PropertyList>
