<?xml version="1.0"?>
<PropertyList>

<path>jrb-usn-mag-compass.ac</path>

<animation>
  <type>material</type>
  <object-name>Housing</object-name>
  <object-name>Ring</object-name>
  <emission>
  <red>   0.6 </red>
  <green> 0.3 </green>
  <blue>  0.3 </blue>
  <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
  </emission> 
</animation>

<animation>
   <type>noshadow</type>
</animation>

<animation>
   <type>alpha-test</type>
   <alpha-factor>0.01</alpha-factor>
</animation>

<animation>
   <type>range</type>
   <min-m>0</min-m>
   <max-m>5</max-m>
</animation>

