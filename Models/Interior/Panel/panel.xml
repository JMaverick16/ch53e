    <animation>
        <type>knob</type>
        <object-name>PFltInstDial</object-name>
        <property>controls/lighting/flight-instruments-norm</property>
        <factor>-300</factor>
        <center>
            <x-m>0.5302</x-m>
            <y-m>0.4049</y-m>
            <z-m>0.6685</z-m>
        </center>
        <axis>
            <x1-m>0.5302</x1-m>
            <y1-m>0.4049</y1-m>
            <z1-m>0.6685</z1-m>
            <x2-m>0.5316</x2-m>
            <y2-m>0.4049</y2-m>
            <z2-m>0.6621</z2-m>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/flight-instruments-norm</property>
                <factor>1</factor>
                <min>0</min>
                <max>4</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
