<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-30.0</chase-distance-m>

  <view n="100">
    <name>Co Pilot Seat View</name>
	<type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
     <x-offset-m archive="y">  -0.68 </x-offset-m> <!--right-->
      <y-offset-m archive="y">  2.1 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -6.3 </z-offset-m> <!--back-->
      <default-field-of-view-deg>85</default-field-of-view-deg>
      <dynamic-view type="bool">true</dynamic-view>
    </config>
  </view>

  <view n="101">
    <name>Bay View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y" type="double"> 0.3 </x-offset-m> <!--Right-->
      <y-offset-m archive="y" type="double"> 1.7 </y-offset-m> <!--Up-->
      <z-offset-m archive="y" type="double"> 5.3 </z-offset-m> <!--Back-->
      <pitch-offset-deg>-10</pitch-offset-deg>
      <default-field-of-view-deg>60</default-field-of-view-deg>
      <dynamic-view type="bool">true</dynamic-view>
    </config>
  </view>

  <view n="102">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  5 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 16.0 </z-offset-m> <!--Back-->
    </config>
  </view>

